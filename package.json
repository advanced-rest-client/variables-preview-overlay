{
  "name": "variables-preview-overlay",
  "version": "0.1.6",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element to display quick preview of variables values for selected environment",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "private": false,
  "main": [
    "variables-preview-overlay.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint variables-preview-overlay.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"variables-preview-overlay:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/variables-preview-overlay.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/variables-preview-overlay/issues",
    "email": "arc@mulesoft.com"
  }
}